{
  "name": "react-demo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "yarn && vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate-typed-schema": "yarn ts-graphql-plugin typegen",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --write {src/**/*.tsx,src/**/*.ts}",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "requirements-check": "bash ./check_node_version.sh",
    "preinstall": "if [ -z \"$NODE_ENV\" ]; then yarn requirements-check; fi"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "lint:fix",
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.11.2",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "@react-oauth/google": "^0.12.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "graphql-crunch": "^2.2.0",
    "jotai": "^2.9.3",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.1",
    "react-router-dom": "^6.11.2",
    "react-toastify": "^10.0.5",
    "react-transition-group": "^4.4.5",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2"
