{
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-json.lock",
    "start": "yarn && node ./test-check-env-equality.js && yarn && lookenv --path=./env/lookenv.dev.js --dotenv=./env/.env -- nodemon ./src/server.js --exec babel-node -r './node_modules/dotenv/config' -e js",
    "add-component-part": "cd ./src/scripts && bash create-graphql-component.sh",
    "remove-component-part": "cd ./src/scripts && bash delete-graphql-component.sh",
    "lint": "eslint src/ --ext js,json",
    "lint-fix": "eslint ./src --ext js,json --fix",
    "build-docs": "yarn spectaql ./spectaql.yaml",
    "build": "babel -d ./build ./src -s",
    "serve-staging": "lookenv -- node ./server.js",
    "serve-production": "lookenv -- node ./server.js",
    "serve-testing": "lookenv --path=./env/lookenv.testing.js --dotenv=./env/.env -- babel-node ./src/server.js",
    "requirements-check": "bash ../checks/check_node_version.sh backend",
    "preinstall": "if [ -z \"$NODE_ENV\" ]; then yarn requirements-check; fi"
  },
  "dependencies": {
    "@brokerloop/ttlcache": "^3.2.3",
